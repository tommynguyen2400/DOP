{
    "Version": "2012-10-17",
    "Statement": 
    [
        {
            "Sid": "FullAccess",
            "Effect": "Allow",
            "Action": ["s3:*"],
            "Resource": ["*"]
        },

        {
            "Sid": "ReadOnlyAccess",
            "Effect": "Deny",
            "Action": [
                "s3:Get*",
                "s3:List*"
            ],
            "Resource": ["*"]
        }
    ]
}